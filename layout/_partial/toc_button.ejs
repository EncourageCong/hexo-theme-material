<% if(theme.scheme === 'Paradox') { %>
    <!-- Left aligned menu below button -->
    <% var toc_obj = toc(page.content, {
        class: 'post-toc',
        list_number: theme.toc.linenumber || false,
    }) %>
    <% if(toc_obj.length > 0) { %>
    <ul class="post-toc-wrap mdl-menu mdl-menu--bottom-left mdl-js-menu mdl-js-ripple-effect" id="toc_button" for="post-toc-trigger-btn" style="position: inherit;">
        <%- toc_obj %>
    </ul>
    <% } %>
<% } %>
